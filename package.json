  "bugs": {
    "url": "https://github.com/JorgenVatle/meteor-type-validation/issues"
  },
  "homepage": "https://github.com/JorgenVatle/meteor-type-validation#readme",
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@types/lodash-es": "^4.17.12",
    "@types/meteor": "^2.9.8",
    "lodash-es": "^4.17.21",
    "tsup": "^8.0.2",
    "typescript": "^5.7.3",
    "vitest": "^3.0.6"
